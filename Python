import boto3
import botocore

def read_s3_data(bucket_name, object_key, aws_access_key_id=None, aws_secret_access_key=None, region_name=None):
    # Initialize a session using Amazon S3
    session = boto3.Session(
        aws_access_key_id=aws_access_key_id,
        aws_secret_access_key=aws_secret_access_key,
        region_name=region_name
    )

    # Create an S3 client
    s3 = session.client('s3')

    try:
        # Fetch the object from the bucket
        obj = s3.get_object(Bucket=bucket_name, Key=object_key)
        # Read the object's content
        data = obj['Body'].read().decode('utf-8')
        return data
    except botocore.exceptions.ClientError as e:
        # Error handling
        print(f"Error reading object {object_key} from bucket {bucket_name}. Error: {e}")
        return None

if _name_ == "_main_":
    # Replace these variables with your actual values
    bucket_name = 'your-bucket-name'
    object_key = 'path/to/your/object.txt'
    aws_access_key_id = 'your-access-key-id'
aws_secret_access_key = 'your-secret-access-key'
    region_name = 'your-region-name'

    # Read data from S3
    data = read_s3_data(bucket_name, object_key, aws_access_key_id, aws_secret_access_key, region_name)
    
    if data:
        print("Data read from S3:")
        print(data)
